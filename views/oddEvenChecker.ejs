<%- include('partials/head') <body>
    <%- include('partials/navbar') %>
        <h1> Your random number is : <%= num %>
        </h1>
        <% if (num> 1 && num % 2 === 0) { %>
            <p>
                It is even number
            </p>
            <% } else { %>
                <p>
                    It is odd number
                </p>
                <% } %>
                    <h3>Number is <%= num%2===0? "EVEN" : "ODD" %>
                    </h3>
                    <%- include('partials/footer')%>